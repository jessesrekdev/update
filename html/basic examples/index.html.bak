      onmouseout="this.style.backgroundColor='#f0f0f0'; this.style.color='#3498db';">
      <svg xmlns="http://www.w3.org/2000/svg" fill="none" stroke="#3498db" stroke-width="2.5" stroke-linecap="round"
        stroke-linejoin="round" width="20" height="20" viewBox="0 0 24 24">
        <polyline points="15 18 9 12 15 6"></polyline>
      </svg>
      Preview
    </button>

    <!-- Continue Button -->
    <button id="next" style="
    display: flex; 
    align-items: center; 
    gap: 8px; 
    background-color: #3498db; 
    border: 2px solid #3498db; 
    color: #fff; 
    padding: 12px 22px; 
    border-radius: 30px; 
    font-weight: 600; 
    font-size: 1rem; 
     
    transition: background-color 0.3s ease, color 0.3s ease;
  " onmouseover="this.style.backgroundColor='#2874a6';" onmouseout="this.style.backgroundColor='#3498db';">
      Continue
      <svg xmlns="http://www.w3.org/2000/svg" fill="none" stroke="#fff" stroke-width="2.5" stroke-linecap="round"
        stroke-linejoin="round" width="20" height="20" viewBox="0 0 24 24">
        <polyline points="9 18 15 12 9 6"></polyline>
      </svg>
    </button>

    <script>
document.getElementById("next").addEventListener("click", function () {
  window.open('../elements', '_self');
});
document.getElementById("preview").addEventListener("click", function () {
  window.open('../editors', '_self');

});


    </script>

  </div>

  <footer id="footer"></footer>
  </footer>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.11.4/gsap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.11.4/ScrollTrigger.min.js"></script>
  
  <script>
    // GSAP Animations
    document.addEventListener('DOMContentLoaded', () => {
      // Register ScrollTrigger plugin
            gsap.registerPlugin(ScrollTrigger);

      // Function to make elements visible as they scroll into view
      function animateOnScroll() {
        const sections = document.querySelectorAll('.content-section');

        sections.forEach(section => {
          gsap.fromTo(section, 
            { opacity: 0, y: 30 },
            { 
              opacity: 1, 
              y: 0, 
              duration: 0.8,
              ease: "power2.out",
              scrollTrigger: {
                trigger: section,
                start: "top 85%", 
                toggleActions: "play none none none"
              }
            }
          );
        });
      }

      // Initial call to set up the animations
      animateOnScroll();

      // Theme Toggle Functionality
      const toggleThemeBtn = document.getElementById('toggleTheme');
      const themeIcon = document.getElementById('themeIcon');
      const sun = themeIcon.querySelector('.sun');
      const moon = themeIcon.querySelector('.moon');

      let isDark = false;

      toggleThemeBtn.addEventListener('click', () => {
        isDark = !isDark;
        document.body.classList.toggle('dark-mode');

        if (isDark) {
          // Animate to dark mode
          gsap.to(sun, { opacity: 0, duration: 0.5, ease: "power2.in" });
          gsap.to(moon, { opacity: 1, scale: 1, duration: 0.5, ease: "power2.out" });
        } else {
          // Animate to light mode
          gsap.to(sun, { opacity: 1, duration: 0.5, ease: "power2.out" });
          gsap.to(moon, { opacity: 0, scale: 0.7, duration: 0.5, ease: "power2.in" });
        }
      });
      
      // Interactive Demo Functionality
      window.runDemo = function(demoId) {
        const demoResult = document.getElementById(demoId);
        if (demoResult.style.display === 'block') {
          demoResult.style.display = 'none';
        } else {
          demoResult.style.display = 'block';
        }
      };

    });

    
  </script>
  <script src="/template.js"></script>
  <script src="/script.js"></script>
  
</body>

</html>

