          <rect x="210" y="90" width="140" height="20" rx="2" fill="white"/>
          <rect x="220" y="95" width="120" height="10" rx="2" fill="#E8EAED"/>
          <rect x="210" y="120" width="140" height="80" rx="2" fill="white"/>
          <rect x="220" y="130" width="120" height="10" rx="2" fill="#E8EAED"/>
          <rect x="220" y="150" width="100" height="10" rx="2" fill="#E8EAED"/>
          <rect x="220" y="170" width="80" height="10" rx="2" fill="#E8EAED"/>
          
          <!-- Inspector Icon -->
          <circle cx="370" cy="55" r="8" fill="#4285F4"/>
          <path d="M365 50 L375 50 L375 60 L365 60 Z" fill="white"/>
          <path d="M367 52 L373 52 L373 58 L367 58 Z" fill="#4285F4"/>
        </svg>
      </div>
    </section>

    <section class="content-section" style="text-align: center; background: linear-gradient(135deg, #4285F4, #EA4335); color: white;">
      <h2 style="font-size: 2rem; margin-bottom: 20px;">Ready to Practice?</h2>
      <p style="font-size: 1.2rem; max-width: 700px; margin: 0 auto 30px;">Try our interactive HTML editor with live preview to experiment with these examples and more!</p>
      
      <button class="primary-btn" style="background: white; color: #4285F4;">
